{
  "name": "@uuv/playwright",
  "version": "1.2.0-beta-20230417",
  "type": "commonjs",
  "author": "Louis Fredice NJAKO MOLOM (https://gitlab.com/curacao) & Stanley SERVICAL (https://gitlab.com/stanleyservical)",
  "description": "A transpiler from cucumber step definitions to cypress.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:e2e-testing/uuv.git"
  },
  "keywords": [ "uuv", "UUV" ,"E2E", "test", "testing", "playwright", "testing-library", "cucumber", "gherkin"],
  "scripts": {
    "generate:step-definitions": "ts-node generate-step-definitions.ts",
    "package": "npm pack --pack-destination=\"../../dist/packages\"",
    "postinstall": "node postinstall.js",
    "serverTest:run": "ts-node playwright/run-test-app.ts 9002",
    "test:run": "npx bddgen -o tests/.features-gen && npx playwright test",
    "test:clean:run": "npx playwright test"
  },
  "bin": {
    "uuv": "./src/lib/uuv-cli.js"
  },
  "dependencies": {
    "axe-core": "4.6.3",
    "chalk": "4.1.2",
    "cucumber-json-report-formatter": "0.1.4",
    "figlet": "1.5.2",
    "is-admin": "2.1.1",
    "minimist": "1.2.8",
    "multiple-cucumber-html-reporter": "3.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*",
    "postinstall.js",
    "target-config/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cypress/playwright.config": {
      "import": "./dist/playwright/playwright.config.js",
      "require": "./dist/playwright/playwright.config.js",
      "types": "./dist/playwright/playwright.config.d.ts"
    },
    "./cucumber/step_definitions": {
      "import": "./dist/cucumber/step_definitions/**/*.js",
      "require": "./dist/cucumber/step_definitions/**/*.js",
      "types": "./dist/cucumber/step_definitions/**/*.d.ts"
    }
  },
  "devDependencies": {
    "@types/minimist": "1.2.2",
    "@types/node": "18.15.11",
    "@uuv/runner-commons": "1.2.0-beta-20230417",
    "cross-env": "7.0.3",
    "serve-handler": "6.1.5",
    "ts-loader": "9.4.2",
    "tsconfig-paths": "4.2.0",
    "webpack": "5.76.1",
    "@cucumber/cucumber": "9.1.0",
    "@playwright/test": "1.32.3",
    "axe-playwright": "1.2.3",
    "playwright": "1.32.3",
    "playwright-bdd": "2.0.0"
  }
}
